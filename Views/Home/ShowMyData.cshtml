@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="~/MyContent/images/ic_launcher.png" rel="shortcut icon" />
    <!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
    <title>我的数据</title>

    <!-- Bootstrap -->

    <link href="~/Content/themes/base/minified/bootstrap.min.css" rel="stylesheet" />
    <link href="~/MyContent/css/mycss.css" rel="stylesheet" />
    <link href="~/MyContent/css/myIndexcss.css" rel="stylesheet" />
    <link href="~/MyContent/css/base.css" rel="stylesheet" />
    <link href="~/MyContent/css/demo.css" rel="stylesheet" />
    <link href="~/MyContent/css/default.css" rel="stylesheet" />
    <style type="text/css">
        .mychat {
            /*border-style:solid;
            border-width:1px;*/
        }
        body {
            /*background-color:#CCFFCC;*/
        }


        .mylabel1 {
            font-size: 20px;
        }

       .value{
            font-size:25px;
            color:#FF9999;
        }

        .newdata {
            /*border-bottom-style:solid;
            border-top-style:solid;
            border-width:1px;*/
            background-color:#FFFFCC;
        }
    </style>


</head>
<body>
    <!-------------------------------------头部导航开始-------------------------------------------------->
    @RenderPage("../Shared/Header.cshtml")
    <!-------------------------------------头部导航结束-------------------------------------------------->

    <!-------------------------------------body内容开始-------------------------------------------------->
    <div style="margin-top:30px;margin-left:10px;margin-right:10px;">
        <div class="row newdata">
            <div id="s1" class="col-md-2 col-xs-2"><span class="mylabel1">心率: </span><span class="value">72</span><span> 次/分</span></div>
            <div id="s2" class="col-md-2 col-xs-2"><span class="mylabel1">体温: </span><span class="value">37</span><span> ℃</span></div>
            <div id="s3" class="col-md-2 col-xs-2"><span class="mylabel1">体重: </span><span class="value">56</span><span> Kg</span></div>
            <div id="s4" class="col-md-2 col-xs-2"><span class="mylabel1">收缩压: </span><span class="value">70</span><span> mmHg</span></div>
            <div id="s5" class="col-md-2 col-xs-2"><span class="mylabel1">舒张压: </span><span class="value">100</span><span> mmHg</span></div>
            @*<div id="s6" class="col-md-2 col-xs-2"><span class="mylabel1">心率:</span><span class="value">72</span><span> 次/分</span></div>*@
        </div>
        <div class="row" style="margin-top:20px;">
            <div id="container1" style="min-width: 310px; height: 400px; margin: 0 auto" class="col-md-6 col-xs-6 mychat"></div>
            <div id="container2" style="min-width: 310px; height: 400px; margin: 0 auto" class="col-md-6 col-xs-6 mychat"></div>
        </div>
         <hr />
            <div class="row">
                <div id="container3" style="min-width: 310px; height: 400px; margin: 0 auto" class="col-md-6 col-xs-6 mychat"></div>
                <div id="container4" style="min-width: 310px; height: 400px; margin: 0 auto" class="col-md-6 col-xs-6 mychat"></div>
            </div>
            <hr />
            <div class="row">
                <div id="container5" style="min-width: 310px; height: 400px; margin: 0 auto" class="col-md-6 col-xs-6 mychat"></div>
                @*<div id="container6" style="min-width: 310px; height: 400px; margin: 0 auto" class="col-md-6 col-xs-6 mychat"></div>*@
            </div>
    </div>
    <!-------------------------------------body内容结束-------------------------------------------------->
    <!-------------------------------------底部内容开始-------------------------------------------------->
    @RenderPage("../Shared/Footer.cshtml")
    <!-------------------------------------底部内容结束-------------------------------------------------->



    <script src="~/MyContent/js/jquery-1.11.2.min.js"></script>
    @*<script src="~/Scripts/jquery-1.8.2.min.js"></script>*@
    <script src="~/MyContent/js/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/highcharts.js"></script>
    <script src="~/Scripts/exporting.js"></script>
    <script src="~/MyContent/js/InitDataChart.js"></script>

   <script type="text/javascript">
       $(function () {
           $('#myModal').on('shown.bs.modal', function () {
               $('#myInput').focus();
           });
           $("ul[class$='nav']").children().eq(4).html("<a href='javascript:void(0)'>我的数据 <span class='sr-only'>(current)</span>").addClass("active");
       });

       function myHelpLink() {
           $(".btn-lg").click();
       }
   </script>
</body>

</html>